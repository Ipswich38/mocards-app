# MOCARDS Enterprise Environment Configuration
# Copy this file to .env and configure with your actual values

# Supabase Configuration (Required)
VITE_SUPABASE_URL=your_supabase_project_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Application Configuration
VITE_APP_NAME=MOCARDS Enterprise
VITE_APP_VERSION=2.0.0
VITE_ENVIRONMENT=development

# Feature Flags - Enterprise Features
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_REALTIME=true
VITE_ENABLE_AUDIT_LOGGING=true
VITE_ENABLE_MULTI_TENANT=true
VITE_ENABLE_SECURITY_MONITORING=true

# Security Configuration
VITE_SESSION_TIMEOUT=3600
VITE_MAX_LOGIN_ATTEMPTS=5
VITE_ACCOUNT_LOCKOUT_DURATION=1800

# Performance Configuration
VITE_API_TIMEOUT=30000
VITE_BATCH_SIZE=100
VITE_CACHE_TTL=300

# Development Configuration
VITE_ENABLE_DEBUG=true
VITE_LOG_LEVEL=info

# Cloud Storage (if using Supabase Storage)
VITE_STORAGE_BUCKET=mocards-files

# External Services (Optional)
VITE_ANALYTICS_ID=
VITE_SENTRY_DSN=
VITE_STRIPE_PUBLIC_KEY=

# Demo Configuration
VITE_DEMO_MODE=true
VITE_DEMO_CLINIC_CODE=DEMO001
VITE_DEMO_PASSWORD=demo123